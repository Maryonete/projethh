<form
  method="post"
  action="{{ path('asso_delete', { id: association.id }) }}"
  onsubmit="return confirm('Etes vous sur de vouloir supprimer cette association?');"
>
  <input
    type="hidden"
    name="_token"
    value="{{ csrf_token('delete' ~ association.id) }}"
  />
  <button class="btn btn-primary btn-block">Supression</button>
</form>
